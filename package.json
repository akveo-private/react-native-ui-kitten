{
  "name": "ui-kitten",
  "version": "4.3.0-beta.1",
  "private": true,
  "license": "MIT",
  "author": "akveo <contact@akveo.com>",
  "homepage": "https://github.com/akveo/react-native-ui-kitten#readme",
  "repository": "git+https://github.com/akveo/react-native-ui-kitten.git",
  "bugs": {
    "url": "https://github.com/akveo/react-native-ui-kitten/issues"
  },
  "workspaces": {
    "packages": [
      "src/*",
      "docs"
    ]
  },
  "scripts": {
    "playground": "yarn --cwd src/playground",
    "docs": "yarn --cwd docs",
    "build": "tsc && tscpaths -p ./tsconfig.json -s ./src/components -o ./dist/tsc-out/components",
    "clean": "rimraf ./dist",
    "docs:parse": "gulp docs",
    "docs:start": "yarn docs:parse && yarn docs start",
    "lint": "tslint -c ./tslint.json ./src/**/*.{ts,tsx}",
    "release:changelog": "conventional-changelog -i CHANGELOG.md -s",
    "release:validate": "yarn lint && yarn test",
    "release:prepare": "yarn clean && yarn build",
    "release:publish": "./scripts/publish.sh",
    "release": "yarn release:validate yarn release:prepare && yarn release:publish",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@eva-design/eva": "^1.3.0-beta.1",
    "@types/gulp": "ts3.5",
    "@types/gulp-replace": "ts3.5",
    "@types/jest": "ts3.5",
    "@types/lodash.merge": "ts3.5",
    "@types/react-native": "ts3.5",
    "@types/react-test-renderer": "ts3.5",
    "@types/rimraf": "ts3.5",
    "babel-jest": "^24.8.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "change-case": "^3.1.0",
    "conventional-changelog-cli": "^2.0.21",
    "coveralls": "^3.0.2",
    "doc-prsr": "^2.2.3",
    "gulp": "^4.0.2",
    "gulp-typedoc": "^2.2.3",
    "husky": "^1.1.2",
    "jest": "^24.8.0",
    "lerna": "^3.19.0",
    "metro-react-native-babel-preset": "^0.54.0",
    "react": "~16.8.3",
    "react-native": "~0.59.10",
    "react-native-svg": "~9.9.5",
    "react-native-testing-library": "^1.5.0",
    "react-test-renderer": "16.8.3",
    "rimraf": "^2.6.2",
    "ts-node": "^3.2.2",
    "tscpaths": "0.0.9",
    "tslint": "^5.12.1",
    "typedoc": "^0.15.3",
    "typescript": "^3.5.1"
  },
  "resolutions": {
    "react": "~16.8.3",
    "react-native": "~0.59.10",
    "react-native-svg": "~9.9.5"
  }
}


