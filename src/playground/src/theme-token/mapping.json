{
  "Sample": {
    "parameters": [
      "backgroundColor",
      "textColor"
    ],
    "states": [
      "active",
      "disabled"
    ],
    "variants": {
      "default": {
        "backgroundColor": "background-color-sample-default",
        "textColor": "text-color-sample-default",
        "state": {
          "active": {
            "backgroundColor": "background-color-sample-dark",
            "textColor": "text-color-sample-dark"
          },
          "disabled": {
            "textColor": "text-color-sample-default-disabled"
          }
        }
      },
      "dark": {
        "backgroundColor": "background-color-sample-dark",
        "textColor": "text-color-sample-dark",
        "state": {
          "active": {
            "backgroundColor": "background-color-sample-default",
            "textColor": "text-color-sample-default"
          }
        }
      },
      "success": {
        "textColor": "text-color-sample-success",
        "state": {
          "active": {
            "backgroundColor": "background-color-sample-default",
            "textColor": "text-color-sample-success-active"
          },
          "disabled": {
            "backgroundColor": "background-color-sample-success-disabled"
          }
        }
      }
    }
  },
  "Radio": {
    "parameters": [
      "size",
      "borderWidth",
      "tintColor",
      "borderColor"
    ],
    "variants": {
      "default": {
        "size": "size-radio-medium",
        "borderWidth": "border-width-radio-medium",
        "tintColor": "tint-color-radio-default",
        "borderColor": "border-color-radio-default"
      },
      "small": {
        "size": "size-radio-small",
        "borderWidth": "border-width-radio-small"
      },
      "large": {
        "size": "size-radio-large",
        "borderWidth": "border-width-radio-large"
      },
      "success": {
        "tintColor": "tint-color-radio-success",
        "borderColor": "border-color-radio-success"
      },
      "info": {
        "tintColor": "tint-color-radio-info",
        "borderColor": "border-color-radio-info"
      },
      "warning": {
        "tintColor": "tint-color-radio-warning",
        "borderColor": "border-color-radio-warning"
      },
      "danger": {
        "tintColor": "tint-color-radio-danger",
        "borderColor": "border-color-radio-danger"
      }
    }
  }
}
