{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/tsc-out",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "jsx": "react-native",
    "target": "es2015",
    "lib": [
      "es2018"
    ],
    "typeRoots": [
      "./node_modules/@types"
    ],
    "paths": {
      "@kitten/*": ["./src/framework/*"]
    }
  }
}
